{
  "name": "vidtalk",
  "version": "1.0.0",
  "description": "Vidtalk with local functionality",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "client": "cd client && npm start",
    "server": "cd server && npm run dev",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
